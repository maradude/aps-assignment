   "source": [
    "data Expr = Value Int | Add Expr Expr | Sub Expr Expr\n",
    "eval :: Expr -> Int\n",
    "eval (Value v) = v\n",
    "eval (Add e1 e2) = (eval e1) + (eval e2)\n",
    "eval (Sub e1 e2) = (eval e1) - (eval e2)"
   ]
  },
